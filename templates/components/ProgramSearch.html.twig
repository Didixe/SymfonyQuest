<div{{ attributes }}>
    <div class="mb-3">
        <label class="form-label" for="search">
            Rechercher une vid√©o
        </label>
        <input type="search"
               id="search"
               class="form-control"
               value="{{ query }}"
               data-model="query">
    </div>
    {% for program in this.programs %}
        <div>
            <h2 class="mt-4"> {{ program.id }} / {{ program.title }} -
                Category: {{ program.category.name }}</h2>
            <p>{{ program.synopsis }}</p>
            <a href="{{ path("program_show", { slug: program.slug }) }}"> see more </a>
        </div>
    {% endfor %}
</div>
